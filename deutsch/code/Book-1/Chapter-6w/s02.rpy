# game/code/Book-1/Chapter-6w/s02.rpy:238
translate deutsch b1c06ws02_2f97ab8d:

    # sm "*Laughing* Let's get inside."
    sm "*Lachend* Gehen wir rein."

# game/code/Book-1/Chapter-6w/s02.rpy:241
translate deutsch b1c06ws02_0c04f040:

    # mct "She is really growing to trust me."
    mct "Sie fängt wirklich an, mir zu vertrauen."

# game/code/Book-1/Chapter-6w/s02.rpy:243
translate deutsch b1c06ws02_faba05b9:

    # mct "Oh. She's...really growing to trust me..."
    mct "Oh. Sie... fängt wirklich an, mir zu vertrauen..."

